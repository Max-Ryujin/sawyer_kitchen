<mujocoinclude>
    <compiler inertiafromgeom="auto" inertiagrouprange="4 4" angle="radian"/>

    <asset>
        <mesh file="../meshes/cup.stl" name="cup" scale="1.5 1.5 1.5"/>
        <mesh file="../meshes/handle.stl" name="handle" scale="1.5 1.5 1.5"/>

        <material name="cmwhite" rgba=".9 .9 .9 1" reflectance="1" shininess="1" />
    </asset>
    <default>
        <default class="cup">
            <joint damping="2" frictionloss="2" armature=".01" limited="true" />
            <geom conaffinity="0" contype="0" group="1" type="mesh"/>
        </default>
        <default class="cm_col">
            <geom conaffinity="3" condim="4" contype="3" group="4" margin="0.001" solimp=".8 .9 .01" solref=".02 1"/>
        </default>
    </default>
</mujocoinclude>